{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "declaration": true,
    "isolatedModules": true,
    "noEmit": true,
    "importHelpers": true,
    "jsx": "react"
  },
  "include": [
    "src",
    "src/components/OptionsPage/WatercolorCircles.js",
    "src/components/OptionsPage/OptionsForm.js"
  ],
  "exclude": [
    "node_modules",
    "build"
  ]
}
